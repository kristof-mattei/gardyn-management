{
    "debug.internalConsoleOptions": "openOnSessionStart",
    "prettier.configPath": "./prettier.config.mjs",
    "rust-analyzer.debug.engineSettings": {
        "lldb": {
            "internalConsoleOptions": "openOnSessionStart",
            "terminal": "console"
        }
    },
    "rust-analyzer.debug.openDebugPane": true,
    "rust-analyzer.runnables.extraEnv": {
        "PROXY_FRONT_END": "127.0.0.1:4000",
        "RUST_LOG": "INFO,socketioxide=INFO,engineioxide=INFO,gardyn_management=TRACE"
    },
    "Prettier-SQL.keywordCase": "upper",
    "Prettier-SQL.SQLFlavourOverride": "sql",
    "Prettier-SQL.commaPosition": "before"
}
